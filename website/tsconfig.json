{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"]
    },
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["src/**/*", "libs/**/*", "docusaurus.config.ts", "jest.config.ts", "__mocks__/**/*"],
  "exclude": ["node_modules", "build"]
}
